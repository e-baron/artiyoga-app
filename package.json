{
  "name": "jam-site",
  "version": "0.1.0",
  "private": true,
  "main": "dist-electron/main/main.js",
  "scripts": {
    "next:dev": "next dev -p 3001",
    "watch:main": "tsc -p tsconfig.main.json -w",
    "dev": "concurrently -k -r -n NEXT,MAIN,ELECTRON \"npm:next:dev\" \"npm:watch:main\" \"wait-on tcp:3001 && electron .\"",
    "build:next": "next build",
    "build:main": "tsc -p tsconfig.main.json",
    "build": "npm run build:next && npm run build:main",
    "start": "electron .",
    "dist": "electron-builder"
  },
  "dependencies": {
    "@codemirror/lang-markdown": "^6.3.4",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.6",
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.1",
    "@mui/icons-material": "^7.1.2",
    "@mui/material": "^7.1.2",
    "@mui/material-nextjs": "^7.1.1",
    "@next/mdx": "^15.3.4",
    "@types/lodash": "^4.17.19",
    "@types/mdx": "^2.0.13",
    "@types/plantuml-encoder": "^1.4.2",
    "@uiw/react-codemirror": "^4.25.2",
    "contentlayer2": "^0.5.8",
    "date-fns": "^4.1.0",
    "fs-extra": "^11.3.2",
    "gh-pages": "^6.3.0",
    "isomorphic-git": "^1.33.2",
    "loadash": "^1.0.0",
    "lodash": "^4.17.21",
    "material-symbols": "^0.35.0",
    "next": "^15.5.3",
    "next-contentlayer2": "^0.5.8",
    "plantuml-encoder": "^1.4.0",
    "prism-react-renderer": "^2.4.1",
    "prism-themes": "^1.9.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-responsive-carousel": "^3.2.23",
    "rehype-prism-plus": "^2.0.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "simple-git": "^3.28.0"
  },
