    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^32.3.3",
    "electron-builder": "^26.0.12",
    "eslint": "^9",
    "eslint-config-next": "^15.5.3",
    "typescript": "^5",
    "wait-on": "^9.0.1"
  }
